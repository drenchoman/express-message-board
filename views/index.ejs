<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <a href="/new"><em>Send a message</em></a>
    <ul>
      <% messages.forEach((message) => { %>
        <li><%= message.text %> <em>from <%= message.user %></em> </li>
        <li><%= message.added %> </li>
        <br />
      <% }) %>
    </ul>
  </body>
</html>
